{
    "ChannelDir": ".",

    "Log": {
        "Level": "info",
        "LogPath": "/var/log",
        "LogFilePrefix": "dappvpn-client"
    },

    "Monitor": {
        "Addr": "localhost:7505",
        "ByteCountPeriod": 5
    },

    "MonitorDelay": 1000,

    "OpenVPN": {
        "Name": "openvpn",
        "Args": [],
        "ConfigRoot": "/etc/openvpn/config",
        "StartDelay": 1000
    },

    "Pusher": {
        "CaCertPath": "/etc/openvpn/config/ca.crt",
        "ConfigPath": "/etc/openvpn/config/server.conf",
        "ExportConfigKeys": [ "proto", "cipher", "ping-restart", "ping",
            "connect-retry", "ca", "comp-lzo"],
        "TimeOut": 120
    },

    "Server": {
        "Addr": "localhost:8000",
        "Password": "secret",
        "TLS": null,
        "Username": "4b26dc82-ffb6-4ff1-99d8-f0eaac0b0532"
    }
}
